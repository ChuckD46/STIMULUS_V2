<HeaderEtudiant sidebarExpandedEtudiant=sidebarExpandedEtudiant SidebarExpandedChangedEtudiant="ToggleSidebarEtudiant" />
<MenuSidebarEtudiant sidebarExpandedEtudiant=sidebarExpandedEtudiant />

@code {
    /// <summary>
    /// Define if the sidebar should be extended when the page is first loaded.
    /// Default to false.
    /// </summary>
    [Parameter]
    public bool sidebarExpandedEtudiant { get; set; } = false;

    private void ToggleSidebarEtudiant(bool HeaderIsExpand)
    {
        sidebarExpandedEtudiant = HeaderIsExpand;
    }
}
